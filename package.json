{
  "name": "@gjsify/generated-docs",
  "version": "0.0.1",
  "description": "Generated Typedoc documentation using @gjsify/docs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently 'npm:start:*'",
    "start:gjs": "typedoc-server serve ./gjs --port 3201 --origin '*'",
    "start:node-gtk": "typedoc-server serve ./node-gtk --port 3202 --origin '*'",
    "start:try-gjs": "typedoc-server serve ./try-gjs --port 3203 --origin '*'",
    "start:try-node-gtk": "typedoc-server serve ./try-node-gtk --port 3204 --origin '*'",
    "convert:search:gjs": "typedoc-server convert ./gjs/assets/search.js --target ./gjs/assets/search.json.7z",
    "convert:search:node-gtk": "typedoc-server convert ./node-gtk/assets/search.js --target ./node-gtk/assets/search.json.7z"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gjsify/generated-docs.git"
  },
  "keywords": [
    "GJS",
    "node-gtk",
    "documentation",
    "typedoc",
    "typescript",
    "gjsify"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "SEE LICENSE IN README.md",
  "bugs": {
    "url": "https://github.com/gjsify/generated-docs/issues"
  },
  "homepage": "https://github.com/gjsify/generated-docs#readme",
  "dependencies": {
    "@gjsify/typedoc-plugin-remote-search": "^0.2.2",
    "concurrently": "^8.2.0"
  }
}
